[mysqld]
   user = {{ mysql_user | default("mysql") }}
   pid-file	= {{ mysql_pid }}
   socket = {{ mysql_socket }}
   port = {{ mysql_port }}
   datadir = {{ mysql_datadir }}
   bind-address = {{ mysql_bind }}


   
